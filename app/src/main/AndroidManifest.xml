<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.citiclubapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LegalPersonCertificateActivity.EnterpriseCertification"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".LegalPersonCertificateActivity.StorageCertification"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".successfulRegis.SuccessfulRiges"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            ></activity>
        <activity
